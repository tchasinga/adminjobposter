generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Make a user model with a unique email
model User {
  id       String  @id @default(cuid())
  name     String?
  email    String  @unique
  password String
}

